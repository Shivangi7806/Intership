    private void t1ActionPerformed(java.awt.event.ActionEvent evt) {                                   

        // TODO add your handling code here:
    }                                  

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         

        try{
            String n=(t1.getText());
            double eng,math,hindi,sci,sst;
            math=Integer.parseInt(t2.getText());
            sci=Integer.parseInt(t3.getText());
            eng=Integer.parseInt(t4.getText());
            hindi=Integer.parseInt(t5.getText());
            sst=Integer.parseInt(t6.getText());
            double total=math+sci+hindi+sst+eng;
            t7.setText(""+total);
        }
        catch(Exception e)
        {
            JOptionPane.showMessageDialog(null,"please enter all the fields...","Msg",1);
        }
        // TODO add your handling code here:
    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        try{
            double perc=Double.parseDouble(t8.getText());
            if(perc>=90)
            {
                t9.setText("A");
              t10.setText("OUTSTANDING ");
           
            }
            else if(perc>=70)
            {
                t9.setText("B");
                 t10.setText("GOOD");
            }
            else if(perc>=50)
            {
                t9.setText("c");
                 t10.setText("AVERAGE");
            }
            else if(perc>=33)
            {
                t9.setText("D");
                 t10.setText("NEED IMPROVEMENT ");
            }
            else
            {
                t9.setText("FAIL");
            }
        }
        catch(Exception e)
        {
            JOptionPane.showMessageDialog(null,"please enter all the fields...","Msg",1);
        }

        // TODO add your handling code here:
    }                                        

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        try{
            double total=Double.parseDouble(t7.getText());
             double perc=total/5;
            t8.setText(""+perc);
        }
        catch(Exception e)
        {
            JOptionPane.showMessageDialog(null,"please enter all the fields...","Msg",1);
        }

        // TODO add your handling code here:
    }                                        

